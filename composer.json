{
  "name": "wellcommerce/core-bundle",
  "type": "wellcommerce-bundle",
  "license": "MIT",
  "minimum-stability": "dev",
  "description": "WellCommerce CoreBundle",
  "keywords": [
    "wellcommerce",
    "e-commerce",
    "open-source"
  ],
  "authors": [
    {
      "name": "Adam Piotrowski",
      "email": "adam@wellcommerce.org"
    },
    {
      "name": "WellCommerce Developers",
      "email": "developers@wellcommerce.org"
    }
  ],
  "support": {
    "email": "support@wellcommerce.org",
    "issues": "https://github.com/WellCommerce/CoreBundle/issues",
    "source": "https://github.com/WellCommerce/CoreBundle/"
  },
  "autoload": {
    "psr-4": {
      "WellCommerce\\Bundle\\CoreBundle\\": "src/WellCommerce/Bundle/CoreBundle"
    }
  },
  "require": {
    "php": "^7.0",
    "symfony/symfony": "v3.2.2",
    "doctrine/orm": "v2.5.4",
    "wellcommerce/collections": "^0.7@dev",
    "wellcommerce/breadcrumb": "^0.7@dev",
    "wellcommerce/datagrid": "^0.7@dev",
    "wellcommerce/dataset": "^0.7@dev",
    "wellcommerce/doctrine-enhancer": "^0.7@dev",
    "wellcommerce/form": "^0.7@dev",
    "wellcommerce/serializer": "^0.7@dev"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.7.x-dev"
    },
    "wellcommerce-bundle": {
      "install-dir": "src/WellCommerce/Bundle/CoreBundle"
    }
  }
}
